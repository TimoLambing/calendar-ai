{
  "name": "calendar-server",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env dotenv -e .env.dev -- nodemon --watch src --ext ts --exec tsx src/index.ts",
    "check": "tsc --noEmit",
    "build": "cross-env dotenv -e .env.production -- tsc",
    "start": "cross-env dotenv -e .env.production -- node dist/index.js",
    "postinstall": "npx prisma generate"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "6.4.1",
    "date-fns": "^4.1.0",
    "dotenv-cli": "^8.0.0",
    "ethers": "^6.13.5",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "moralis": "^2.27.2",
    "nodemon": "^3.1.9",
    "prisma": "^6.4.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "20.16.11",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "cross-env": "^7.0.3",
    "tsx": "^4.19.1",
    "typescript": "^5.7.3"
  }
}
